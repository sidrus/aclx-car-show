<p id="notice"><%= notice %></p>

<h1>Listing Car Shows</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_shows.each do |car_show| %>
      <tr>
        <td><%= car_show.start %></td>
        <td><%= car_show.end %></td>
        <td><%= link_to 'Show', car_show %></td>
        <td><%= link_to 'Edit', edit_car_show_path(car_show) %></td>
        <td><%= link_to 'Destroy', car_show, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car show', new_car_show_path %>
